{
  "name": "todo2",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "security:lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "security:audit": "npm audit --audit-level moderate",
    "security:check": "npm run security:lint && npm run security:audit",
    "security:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "security:transmission": "tsx src/security/scripts/run-transmission-analysis.ts",
    "security:transmission:quick": "tsx src/security/scripts/run-transmission-analysis.ts --quick",
    "security:transmission:report": "tsx src/security/scripts/run-transmission-analysis.ts --verbose --output data-transmission-security-report.md --format markdown",
    "security:csp": "tsx src/security/scripts/run-csp-analysis.ts",
    "security:csp:verbose": "tsx src/security/scripts/run-csp-analysis.ts --verbose",
    "security:csp:report": "tsx src/security/scripts/run-csp-analysis.ts --verbose --output csp-resource-security-report.md --format markdown",
    "security:update": "tsx src/security/scripts/run-update-integrity-analysis.ts",
    "security:update:verbose": "tsx src/security/scripts/run-update-integrity-analysis.ts --verbose",
    "security:update:report": "tsx src/security/scripts/run-update-integrity-analysis.ts --verbose --output=update-integrity-security-report.md --format=markdown",
    "security:privilege": "tsx src/security/run-privilege-tests.ts",
    "security:privilege:setup": "tsx src/security/run-privilege-tests.ts",
    "security:build": "tsx src/security/scripts/run-build-deployment-analysis.ts",
    "security:build:verbose": "tsx src/security/scripts/run-build-deployment-analysis.ts --verbose",
    "security:build:report": "tsx src/security/scripts/run-build-deployment-analysis.ts --verbose --output=security-reports/build-deployment-security-analysis.md --format=markdown",
    "security:deps": "npm run security:audit && npm run security:rust-audit",
    "security:deps:json": "npm run security:audit:json && npm run security:rust-audit:json",
    "security:report": "node scripts/generate-security-report.js",
    "security:rust-audit": "cd src-tauri && cargo audit",
    "security:rust-audit:json": "cd src-tauri && cargo audit --json",
    "security:full-scan": "npm run security:audit && npm run security:rust-audit && npm run security:report",
    "security:update-check": "npm outdated && cd src-tauri && cargo outdated || echo 'Install cargo-outdated with: cargo install cargo-outdated'"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "2.2.0",
    "@heroicons/react": "^2.0.18",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.39.7",
    "@tauri-apps/api": "^2.0.0-alpha.11",
    "@tauri-apps/plugin-dialog": "^2.2.0",
    "clsx": "^2.0.0",
    "date-fns": "4.1.0",
    "framer-motion": "11.18.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@tauri-apps/cli": "2.2.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "22.10.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/ui": "^3.0.7",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "jsdom": "^26.0.0",
    "postcss": "8.5.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.7.0",
    "typescript": "^5.0.2",
    "vite": "6.0.7",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.0.7"
  }
}
